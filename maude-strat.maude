  eq solveBubblesSearch2(M, T, F[T', T''], QI, D, I, VDS)
    = if F == '_s.t._. or F == '_such`that_. 
      then procSearch2(M, T, T', parseCond(T'', VDS), QI, D, I)
      else procSearch2(M, T, constsToVars(F[T', T''], VDS), nil, QI, D, I)
