  eq parseStrat('_`(_`)['token[Q],'bubble[T]], M,VS) =
     call(solveBubbles('bubble['__[Q,''`(.Qid,T,''`).Qid]],setRls(M,none), false, VS, emptyDatabase)) .
     
  
