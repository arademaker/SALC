     matchrew(solveBubbles(T,setRls(M,none), false, VS, emptyDatabase), 
              solveBubblesCond(T', addOps(VS,setRls(M,none)), 
                 addInfoConds(addOps(VS,setRls(M,none))), false, VS, emptyDatabase),
